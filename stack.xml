<stack>
  <description brief="cob_calibration">

    The cob_calibration stack provides tools to automatically calibrate
    the cameras and kinematic chains of Care-O-bot.

  </description>
  <author>Maintained by Sebastian Haug</author>
  <license>BSD,LGPL</license>  
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/cob_calibration</url>
  <depend stack="arm_navigation" /> <!-- kinematics_msgs -->
  <depend stack="cob_command_tools" /> <!-- cob_script_server -->
  <depend stack="cob_robots" /> <!-- cob_default_robot_config, cob_hardware_config -->
  <depend stack="common_msgs" /> <!-- geometry_msgs, sensor_msgs, visualization_msgs -->
  <depend stack="geometry" /> <!-- tf -->
  <depend stack="ros" />
  <depend stack="ros_comm" /> <!-- std_msgs, rosbag, rostest, rospy, message_filters -->
  <depend stack="vision_opencv" /> <!-- cv_bridge -->

</stack>
